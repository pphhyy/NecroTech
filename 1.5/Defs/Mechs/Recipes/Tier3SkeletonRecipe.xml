<!-- Base Recipe -->
<RecipeDef Name="pphhyy_BaseNecroSkeletonTier3" ParentName="pphhyy_BaseNecroSkeleton" Abstract="True">
    <researchPrerequisite>pphhyy_NecroTechBasic</researchPrerequisite>
    <ingredients>
        <li>
            <filter>
                <thingDefs>
                    <li>pphhyy_NecroMetal</li>
                </thingDefs>
            </filter>
            <count>50</count>
        </li>
        <li>
            <filter>
                <thingDefs>
                    <li>pphhyy_VitaeOil</li>
                </thingDefs>
            </filter>
            <count>50</count>
        </li>
        <li>
            <filter>
                <thingDefs>
                    <li>SubcoreRegular</li>
                </thingDefs>
            </filter>
            <count>1</count>
        </li>
    </ingredients>
</RecipeDef>

